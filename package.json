{
  "name": "envisage",
  "version": "0.1.0",
  "description": "An academic advising tool designed to allow students to plan at the speed of thought.",
  "scripts": {
    "serve-ui": "vue-cli-service serve",
    "serve-api": "nodemon --ignore ./node_modules hapi-server.js",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint"
  },
  "dependencies": {
    "@hapi/hapi": "^18.4.0",
    "@hapi/inert": "^5.2.2",
    "@hapi/joi": "^16.1.7",
    "@vue/devtools": "^5.3.3",
    "axios": "^0.19.0",
    "bcrypt": "^3.0.7",
    "blipp": "^4.0.1",
    "core-js": "^3.3.2",
    "firebase": "^7.8.0",
    "googleapis": "39",
    "hapi-pino": "^6.3.0",
    "knex": "^0.20.2",
    "objection": "^1.6.11",
    "pg": "^7.12.1",
    "vue": "^2.6.10",
    "vue-analytics": "^5.22.1",
    "vue-google-login": "^2.0.3",
    "vue-google-signin-button": "^1.0.4",
    "vue-router": "^3.1.3",
    "vuedraggable": "^2.23.2",
    "vuetify": "^2.1.10",
    "vuex": "^3.1.2",
    "vuex-persist": "^2.2.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.0.0",
    "@vue/cli-plugin-eslint": "^4.0.0",
    "@vue/cli-service": "^4.0.0",
    "babel-eslint": "^10.0.3",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "nodemon": "^1.19.4",
    "sass": "^1.19.0",
    "sass-loader": "^8.0.0",
    "vue-cli-plugin-vuetify": "^2.0.2",
    "vue-template-compiler": "^2.6.10",
    "vuetify-loader": "^1.3.0"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true,
      "browser": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/timtheswan/envisage.git"
  },
  "author": "Tim Swanson",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/timtheswan/envisage/issues"
  },
  "homepage": "https://github.com/timtheswan/envisage#readme"
}
